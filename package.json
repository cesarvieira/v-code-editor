{
  "name": "@cesarv/v-code-editor",
  "version": "0.0.1",
  "description": "Componente Vue 3 input para seleção de imagens com Vuetify 3 e TypeScript",
  "main": "dist/v-code-editor.umd.js",
  "module": "dist/v-code-editor.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/v-code-editor.mjs",
      "require": "./dist/v-code-editor.umd.js"
    },
    "./dist/styles.css": "./dist/styles.css"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "scripts": {
    "dev": "vite --config playground/vite.config.ts",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint .",
    "build": "vite build",
    "prepack": "npm run build",
    "preversion": "npm run lint && npm run type-check"
  },
  "keywords": [
    "vue",
    "vue3",
    "vuetify",
    "vuetify3",
    "code",
    "editor",
    "code-editor",
    "html",
    "css",
    "javascript"
  ],
  "author": {
    "name": "Cesar Vieira",
    "email": "cesarvieira.dev@gmail.com"
  },
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesarvieira/v-code-editor.git"
  },
  "bugs": {
    "url": "https://github.com/cesarvieira/v-code-editor/issues"
  },
  "homepage": "https://github.com/cesarvieira/v-code-editor#readme",
  "devDependencies": {
    "@eslint/compat": "^2.0.0",
    "@stylistic/eslint-plugin": "^5.6.1",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-typescript": "^14.6.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-vue": "^10.6.2",
    "sass-embedded": "^1.97.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.51.0",
    "vite": "^7.3.0",
    "vite-plugin-dts": "^4.5.4",
    "vue-tsc": "^3.2.1"
  },
  "peerDependencies": {
    "@codemirror/lang-css": "^6.3.1",
    "@codemirror/lang-html": "^6.4.11",
    "@codemirror/lang-javascript": "^6.2.4",
    "@mdi/font": "^7.4.47",
    "codemirror": "^6.0.2",
    "vue": "^3.5.26",
    "vuetify": "^3.11.5"
  },
  "dependencies": {
    "vue-advanced-cropper": "^2.8.9"
  }
}
